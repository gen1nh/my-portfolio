<+>@import \"tailwindcss\";\r\n\r\n@font-face {\r\n  font-family: \"Maven Pro\";\r\n  src: url(\"/src/assets/fonts/Maven_Pro/MavenPro-Regular.woff2\") format(\"woff2\");\r\n  font-weight: 100 900;\r\n  font-style: normal;\r\n  font-display: swap;\r\n}\r\n\r\n@font-face {\r\n  font-family: \"Poppins\";\r\n  src: url(\"/src/assets/fonts/Poppins/Poppins-SemiBold.woff2\") format(\"woff2\");\r\n  font-weight: 600;\r\n  font-style: normal;\r\n  font-display: swap;\r\n}\r\n\r\n@font-face {\r\n  font-family: \"Poppins\";\r\n  src: url(\"/src/assets/fonts/Poppins/Poppins-Medium.woff2\") format(\"woff2\");\r\n  font-weight: 500;\r\n  font-style: normal;\r\n  font-display: swap;\r\n}\r\n\r\n* {\r\n  margin: 0px;\r\n  padding: 0px;\r\n  box-sizing: border-box;\r\n  font-family: \"Maven Pro\", sans-serif;\r\n}\r\n\r\n.font-primary {\r\n  font-family: \"Poppins\", sans-serif; /* fonte primária */\r\n}\r\n\r\n.font-secondary {\r\n  font-family: \"Maven Pro\", sans-serif; /* fonte secundária */\r\n}\r\n\r\n.icon-skills {\r\n  border-radius: 10px; /* rounded-[10px] */\r\n  opacity: 0.5; /* opacity-50 */\r\n  box-shadow: none; /* shadow-none */\r\n\r\n  transition: all 300ms ease-in-out 70ms; /* transition delay-70 duration-300 ease-in-out */\r\n}\r\n\r\n.icon-skills:hover {\r\n  box-shadow: 0 10px 15px -3px rgba(255, 255, 255, 0.1),\r\n    0 4px 6px -2px rgba(255, 255, 255, 0.05); /* hover:shadow-lg hover:shadow-white/10 */\r\n  transform: translateY(-0.25rem) scale(1.1); /* hover:-translate-y-1 hover:scale-110 */\r\n  opacity: 1; /* hover:opacity-100 */\r\n}\r\n\r\n.swiper {\r\n  width: 100%;\r\n\r\n  /* Adicione estas duas linhas para centralizar o Swiper */\r\n  margin-left: auto;\r\n  margin-right: auto;\r\n  /* Ou, de forma abreviada: margin: 0 auto; */\r\n}\r\n\r\n.swiper-slide {\r\n  display: flex;\r\n  justify-content: center; /* Centraliza o conteúdo do slide horizontalmente */\r\n  align-items: center; /* Centraliza o conteúdo do slide verticalmente */\r\n}\r\n\r\n.swiper-slide img {\r\n  max-width: 100%;\r\n  max-height: 100%;\r\n  height: auto;\r\n  width: auto;\r\n}\r\n\r\n.btn-links {\r\n  padding: 6px 24px;\r\n  transition: ease-in;\r\n  font-weight: 400;\r\n}\r\n\r\n.btn-primary {\r\n  border-radius: 999px;\r\n  width: 168px;\r\n  height: 54px;\r\n  border: 2px solid;\r\n  font-weight: 500;\r\n  font-size: 16px;\r\n}\r\n\r\n.btn-primary:hover {\r\n  background-color: rgba(128, 128, 128, 0.192);\r\n  border-color: white;\r\n  transition: ease-out;\r\n  color: white;\r\n}\r\n\r\n.btn-links-projects {\r\n  align-self: center;\r\n  text-align: center;\r\n  padding: 6px 24px;\r\n  border-radius: 6px;\r\n  color: #e8e8e8;\r\n  transition: ease-out;\r\n  transition-duration: 300ms;\r\n  transform: scale(0px);\r\n}\r\n\r\n.btn-links-projects:hover {\r\n  font-weight: 600;\r\n  background-color: #343a40;\r\n  color: white;\r\n  transition: ease-in;\r\n  transition-duration: 200ms;\r\n}\r\n\r\n.glass {\r\n  background: rgba(217, 217, 217, 0.08);\r\n  -webkit-backdrop-filter: blur(102px);\r\n  backdrop-filter: blur(40px);\r\n  box-shadow: 0 4px 30px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.borda-top-bottom {\r\n  border-top: 2px solid rgba(255, 255, 255, 0.3);\r\n  border-bottom: 2px solid rgba(255, 255, 255, 0.3);\r\n}\r\n.beveled-box {\r\n  border-radius: 0px;\r\n  background-color: #d9d9d914;\r\n  border-top: 1px solid rgba(255, 255, 255, 0.048);\r\n  border-bottom: 1px solid rgba(255, 255, 255, 0.048);\r\n\r\n  box-shadow: inset 0px 0px 2px rgba(0, 0, 0, 0.123);\r\n}\r\n\r\n.skills-box {\r\n  border-radius: 999px;\r\n  background-color: #e6baff33;\r\n  border-top: 1px solid rgba(255, 255, 255, 0.267);\r\n  border-bottom: 1px solid rgba(255, 255, 255, 0.267);\r\n}\r\n\r\n.nav-box {\r\n  background-color: #e6baff33;\r\n  border-top: 1px solid rgba(255, 255, 255, 0.267);\r\n  border-bottom: 1px solid rgba(255, 255, 255, 0.267);\r\n}\r\n\r\n.div-lateral-fade {\r\n  -webkit-mask-image: linear-gradient(to right, transparent, black 10%, black 90%, t